#!/home/nerd/Documents/linux-AutoGrinder/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pyconfig==3.2.3','console_scripts','pyconfig'
__requires__ = 'pyconfig==3.2.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyconfig==3.2.3', 'console_scripts', 'pyconfig')()
    )
